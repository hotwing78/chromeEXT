{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a small icon when clicked",
  "version": "1.0",

  "background": "background.html",

  "browser_action": {
    "popup": "popup.html",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["notifications"]
}
